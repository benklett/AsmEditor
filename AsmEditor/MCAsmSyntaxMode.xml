<SyntaxMode name="MCAsm" mimeTypes="text/x-mcasm">
	<Property name="LineComment">;</Property>
	<Property name="BlockCommentStart">/*</Property>
	<Property name="BlockCommentEnd">*/</Property>
	<Delimiters>&amp;&lt;&gt;~!@%^*()-+=|\#/{}[]:"' ,	?</Delimiters>
	<EolSpan color="Comment(Line)" rule="Comment" tagColor="Comment(Line)">;</EolSpan>
	<Span color="Comment(Block)" rule="Comment" tagColor="Comment(Line)">
		<Begin>/*</Begin>
		<End>*/</End>
	</Span>
	<Span color="String" rule="String" stopateol="true" escape="\">
		<Begin>"</Begin>
		<End>"</End>
	</Span>
	<Span color="String" rule="String" stopateol="true" escape="\">
		<Begin>'</Begin>
		<End>'</End>
	</Span>
	<Match color="Number">CSharpNumber</Match>
	<Keywords color="Keyword(Iteration)">
		<Word>nop</Word>
		<Word>pop</Word>
		<Word>add</Word>
		<Word>sub</Word>
		<Word>mul</Word>
		<Word>muls</Word>
		<Word>div</Word>
		<Word>rem</Word>
		<Word>and</Word>
		<Word>or</Word>
		<Word>xor</Word>
		<Word>lsh</Word>
		<Word>rsh</Word>
		<Word>rsha</Word>
		<Word>not</Word>
		<Word>neg</Word>
		<Word>call</Word>
		<Word>ret</Word>
		<Word>inc</Word>
		<Word>dec</Word>
		<Word>adc</Word>
		<Word>sbb</Word>
		<Word>cmp</Word>
		<Word>mov</Word>
		<Word>ldc</Word>
		<Word>ldrs</Word>
		<Word>ldr</Word>
		<Word>strs</Word>
		<Word>str</Word>
		<Word>push</Word>
		<Word>pusha</Word>
		<Word>pushc</Word>
		<Word>pop</Word>
		<Word>popa</Word>
		<Word>hlt</Word>
		<Word>jmp</Word>
		<Word>je</Word>
		<Word>jz</Word>
		<Word>ja</Word>
		<Word>jae</Word>
		<Word>jnc</Word>
		<Word>jb</Word>
		<Word>jc</Word>
		<Word>jbe</Word>
		<Word>jg</Word>
		<Word>jge</Word>
		<Word>jl</Word>
		<Word>jle</Word>
		<Word>jne</Word>
		<Word>jnz</Word>
		<Word>jns</Word>
		<Word>jo</Word>
		<Word>js</Word>
		<Word>jnp</Word>
		<Word>jpo</Word>
		<Word>jp</Word>
		<Word>jpe</Word>
	</Keywords>
	<Keywords color="Keyword(Type)">
	</Keywords>
	<Keywords color="Keyword(Modifiers)">
	</Keywords>
	<Keywords color="Keyword(Constants)">
	</Keywords>
	<Keywords color="Keyword(Namespace)">
		<Word>a</Word>
		<Word>b</Word>
		<Word>c</Word>
		<Word>d</Word>
	</Keywords>
	<Keywords color="Keyword(Context)">
	</Keywords>
	<Keywords color="Keyword(Access)">
	</Keywords>
	<Rule name="Comment">
		<Keywords color="Comment Tag" ignorecase="True">
			<Word>TODO</Word>
			<Word>FIXME</Word>
			<Word>HACK</Word>
			<Word>UNDONE</Word>
		</Keywords>
	</Rule>
</SyntaxMode>